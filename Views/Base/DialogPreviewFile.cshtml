@model BasePreviewFile
@{
    ViewData["Title"] = "プレビュー";
    Layout = null;
}
<div class="container-fluid">
    @{
        var url = @Url.Content("~/") + "Base/DownloadFile_stream?dic_cd=" + Model.dic_cd + "&dir_no=" + Model.dir_no + "&file_name=" + Model.file_name + "#toolbar=0&navpanes=0";
    }
     <iframe src="@url" width="100%" height="100%"></iframe>
</div>


@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
