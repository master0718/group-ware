@model ConfirmDialogViewModel

@{
    ViewData["Title"] = "Index";
    Layout = "_Layout_headerSidebar_off";

}

<div class="container-fluid">
    <div class="row">
        <div class="col">
            <label>@(Model.Title)を登録をしてよろしいですか？</label>
        </div>
    </div>
    <div class="row text-center">
        <div class="col">
            <a class="text-right btn text-right btn btn-sm btn-primary" asp-controller="Notice" asp-action="Create">はい</a>
            <button id="" class="btn btn-info colorBoxClose">いいえ</button>
        </div>
    </div>

</div>
